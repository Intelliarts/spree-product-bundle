<!-- insert_bottom 'td[data-hook=order_item_description]' -->
<% if item.product.bundle? %>
  <ul class='bundle_parts'>
    <% item.product.parts.each do |v| %>
    <li>(<%= item.product.count_of(v) %>)  <%= link_to v.name, product_path(v.product) %>  (<%= v.sku %>)</li>
    <% end %>
  </ul>
<% end %>
